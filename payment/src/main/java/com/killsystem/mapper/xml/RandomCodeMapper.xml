<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.killsystem.mapper.RandomCodeMapper">
    <select id="findSnowId" resultType="com.killsystem.entity.RandomCode" parameterType="String">
        select code from random_code where code = #{snowId}
    </select>
</mapper>
